@{
}
<div class="row">
    <div class="col-12">
        <section class="panel">
            <div id="message_row">
            </div>
            <div class="form-horizontal form-bordered">
                <div class="table-responsive">
                    <table id="tb-Promociones" class="table table-striped">
                        <thead>
                            <tr>
                                <th>Email</th>
                                <th>Nombre</th>
                                <th>Codigo</th>
                                <th>Estado</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </section>
    </div>
</div>

@section Scripts{
    <script type="text/x-handlebars-template" id="template-filters">

        {{# each response}}
        <tr>
            <td>{{email}}</td>
            <td>{{nombre}}</td>
            <td>{{codigoGenerado}}</td>
            <td>{{promocionEstado.descripcion}}</td>
        </tr>
        {{/each}}

    </script>
}